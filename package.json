{
  "name": "react-project-setup",
  "version": "0.1.0",
  "description": "A project with everything you need to get your react app going.",
  "main": "resources/js/index.js",
  "author": "Lukas Riha",
  "scripts": {
    "dev": "npm run development",
    "start": "npm run development",
    "development": "webpack-dev-server --mode development --color --progress --config ./webpack/webpack.development.js",
    "prod": "npm run production",
    "build": "npm run production",
    "production": "webpack --mode production --color --progress --config ./webpack/webpack.production.js",
    "watch": "webpack --mode development --watch --color --progress --config ./webpack/webpack.development.js",
    "lint": "npm run lint:config && npm run lint:js && npm run lint:sass",
    "lint:config": "eslint ./prettier.config.js ./webpack/ ./.eslintrc ./babel.config.js ./stylelint.config.js ./webpack.input.js --no-ignore --no-eslintrc -c ./webpack/additional/config.eslintrc.js",
    "lint:js": "eslint --ext .jsx,js ./resources/js",
    "lint:sass": "stylelint ./resources/sass",
    "lint:scss": "npm run lint:sass",
    "lint:css": "npm run lint:sass",
    "fix": "npm run fix:config && npm run fix:js && npm run fix:sass",
    "fix:config": "eslint ./prettier.config.js ./webpack/ ./.eslintrc ./babel.config.js ./postcss.js ./stylelint.config.js ./webpack.input.js --no-ignore --fix -c ./webpack/additional/config.eslintrc.js",
    "fix:js": "eslint --ext jsx,js ./resources/js/ --fix",
    "fix:scss": "npm run fix:sass",
    "fix:css": "npm run fix:sass",
    "fix:sass": "stylelint ./resources/sass/ --fix"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^4.2.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "image-webpack-loader": "^6.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.1",
    "raw-loader": "^4.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-webpack-plugin": "^2.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  }
}
